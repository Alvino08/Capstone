APP_ENV=production
APP_DEBUG=false
APP_URL=http://<EC2_PUBLIC_IP>    # Ganti dengan IP publik atau domain EC2 Anda

DB_CONNECTION=mysql
DB_HOST=mysql                     # Sesuaikan dengan nama service di docker-compose
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=root
DB_PASSWORD=password123           # Sesuaikan dengan yang di docker-compose

SESSION_DRIVER=database
SESSION_DOMAIN=<EC2_PUBLIC_IP>    # Bisa diisi domain atau IP publik Anda

CACHE_STORE=file
QUEUE_CONNECTION=sync             # Ganti ke 'sync' jika tidak pakai worker

MAIL_MAILER=smtp
MAIL_HOST=smtp.mailtrap.io        # Atau SMTP server yang digunakan
MAIL_PORT=2525
MAIL_USERNAME=your_username
MAIL_PASSWORD=your_password
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=no-reply@example.com
MAIL_FROM_NAME="${APP_NAME}"
